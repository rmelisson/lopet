<%= form_for(formulario) do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :arguments %><br />
    <%= f.text_area :arguments %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
